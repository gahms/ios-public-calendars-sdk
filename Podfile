target 'iOS-SDK' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS-SDK
  pod 'SchedJoulesApiClient', :git => 'https://github.com/schedjoules/swift-api-client', :branch => '91-No-access-to-calendar'
  pod 'SDWebImage', '~> 4.0'
  
end

